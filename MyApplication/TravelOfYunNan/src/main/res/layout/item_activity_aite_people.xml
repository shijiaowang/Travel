<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:minWidth="@dimen/x100"
    android:layout_width="wrap_content"
    android:orientation="horizontal"
    android:gravity="center_vertical"
    android:layout_height="wrap_content">
    <TextView
        android:id="@+id/tv_aite"
        android:textSize="@dimen/x13sp"
        android:textColor="@color/color7fbdff"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <com.yunspeak.travel.ui.view.FontsIconTextView
        android:id="@+id/tv_delete"
        android:layout_marginLeft="@dimen/x3"
        android:textColor="@color/otherFf7f6c"
        android:text="@string/activity_member_detail_delete"
        android:textSize="@dimen/x10sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</LinearLayout>
