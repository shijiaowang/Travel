<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:orientation="vertical"
    android:gravity="center_horizontal"
    android:layout_height="match_parent">
    <TextView
        android:id="@+id/tv_result"
        android:text="支付成功"
        android:layout_marginTop="@dimen/x57"
        android:textSize="@dimen/x25sp"
        android:textColor="@color/color323232"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <TextView
        android:id="@+id/tv_des"
        android:text="订单等待处理"
        android:textSize="@dimen/x18sp"
        android:textColor="@color/color646464"
        android:layout_marginTop="@dimen/x10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <com.yunspeak.travel.ui.view.FontsIconTextView
        android:id="@+id/tv_pay_icon"
        android:layout_marginTop="@dimen/x42"
        android:textSize="120sp"
        android:textColor="#39B337"
        android:text="@string/activity_confirm_orders_we_chat"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <com.yunspeak.travel.ui.view.AvoidFastButton
        android:layout_marginTop="@dimen/x57"
        android:layout_height="@dimen/x30"
        android:text="返回订单列表"
        android:layout_width="@dimen/x200"
        android:background="@drawable/fragment_find_search_bg"
        style="@style/commonButtonNext" />

</LinearLayout>
