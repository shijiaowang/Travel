<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="316dp"
    android:layout_height="170dp">
    <ImageView
        android:src="@drawable/activity_edit_test"
        android:id="@+id/iv_image"
        android:layout_width="316dp"
        android:layout_height="170dp" />
    <com.example.administrator.travel.ui.view.FontsIconTextView
        android:id="@+id/tv_delete"
        android:layout_margin="20dp"
        android:layout_alignParentRight="true"
        android:textColor="@color/colorb5b5b5"
        android:textSize="15sp"
        android:text="@string/activity_member_detail_delete"
        android:layout_width="20dp"
        android:layout_height="20dp" />
    <TextView
        android:id="@+id/tv_number"
        android:textColor="#fcae04"
        android:textSize="12sp"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="5dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />

</RelativeLayout>
