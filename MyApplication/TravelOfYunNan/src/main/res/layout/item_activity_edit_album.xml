<?xml version="1.0" encoding="utf-8"?>
    <RelativeLayout
        android:layout_width="@dimen/x316"
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_height="@dimen/x170">
    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/iv_image"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
        <com.yunspeak.travel.ui.view.FontsIconTextView
            android:id="@+id/tv_delete"
            android:layout_marginTop="@dimen/x15"
            android:padding="@dimen/x5"
            android:layout_marginRight="@dimen/x10"
            android:layout_alignParentRight="true"
            android:textColor="@color/colorb5b5b5"
            android:textSize="@dimen/x25sp"
            android:text="@string/activity_member_detail_delete"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />
        <TextView
            android:id="@+id/tv_number"
            android:textColor="@color/otherFcae04"
            android:textSize="@dimen/x12sp"
            android:layout_marginLeft="@dimen/x6"
            android:layout_marginTop="@dimen/x5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />


    </RelativeLayout>

