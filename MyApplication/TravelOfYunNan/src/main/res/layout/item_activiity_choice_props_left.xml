<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/x80"
    android:layout_height="@dimen/x50">
 <View
     android:id="@+id/v_cursor"
     android:background="@color/otherTitleBg"
     android:layout_width="@dimen/x3"
     android:layout_height="@dimen/x50"/>
    <TextView
        android:id="@+id/tv_type"
        android:text="登山"
        android:gravity="center"
        android:layout_centerInParent="true"
        android:textColor="@color/color646464"
        android:textSize="@dimen/x18sp"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x50" />
    <com.yunspeak.travel.ui.view.FontsIconTextView
        android:id="@+id/tv_cursor"
        android:textColor="@color/otherTitleBg"
        android:text="@string/activity_cursor_left"
        android:textSize="@dimen/x12sp"
        android:layout_centerVertical="true"
        android:layout_alignParentRight="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</RelativeLayout>
