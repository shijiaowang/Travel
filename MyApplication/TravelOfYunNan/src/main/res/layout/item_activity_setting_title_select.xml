<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:background="@drawable/activity_my_appoint_rl_bg"
        android:layout_width="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginBottom="15dp"
        android:layout_height="wrap_content">
        <TextView
           android:gravity="center"
            android:id="@+id/tv_title"
            android:layout_centerVertical="true"
            android:textColor="@color/otherTitleBg"
            android:paddingLeft="15dp"
            android:textSize="14sp"
            android:layout_width="wrap_content"
            android:layout_height="27dp" />
        <com.example.administrator.travel.ui.view.FontsIconTextView
            android:paddingLeft="3dp"
            android:paddingBottom="3dp"
            android:layout_toRightOf="@id/tv_title"
            android:text="@string/activity_member_detail_delete"
            android:id="@+id/tv_delete"
            android:layout_marginRight="2dp"
            android:textSize="10sp"
            android:textColor="@color/colorFf8076"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content" />

    </RelativeLayout>

</RelativeLayout>
