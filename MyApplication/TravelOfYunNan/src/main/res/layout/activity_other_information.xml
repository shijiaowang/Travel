<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
   android:background="#efeff4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/tv_title_icon"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="12dp"
            android:layout_marginTop="8dp"
            android:text="@string/activity_other_title"
            android:textColor="#5f5f5f"
            android:textSize="13sp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="5dp"
            android:layout_marginTop="8dp"
            android:layout_toRightOf="@id/tv_title_icon"
            android:text="兴趣标签"
            android:textColor="#5f5f5f"
            android:textSize="13sp" />

        <com.example.administrator.travel.ui.view.FlowLayout
            android:id="@+id/fl_title_in_vs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:paddingBottom="20dp"
            android:paddingLeft="26dp"
            android:paddingTop="30dp">

        </com.example.administrator.travel.ui.view.FlowLayout>
    </RelativeLayout>

    <LinearLayout
        android:layout_marginTop="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
       <TextView
           android:layout_marginLeft="12dp"
           android:text="@string/activity_other_detail_message"
           style="@style/activityOtherInformationType"/>
        <TextView
            android:text="基本资料"
            android:layout_marginLeft="10dp"
            style="@style/activityOtherInformationDetail" />

    </LinearLayout>
    <RelativeLayout
        android:layout_marginTop="20dp"
        android:layout_marginBottom="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="昵称"
            style="@style/activityOtherInformationType"/>
        <TextView
            android:text="Magic"
            style="@style/activityOtherInformationDetail" />
    </RelativeLayout>

    <View
        style="@style/activityOtherInformationLine"/>
    <RelativeLayout
        android:layout_marginTop="13dp"
        android:layout_marginBottom="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="性别"
            style="@style/activityOtherInformationType"/>
        <TextView
            android:text="你猜"
            style="@style/activityOtherInformationDetail" />
    </RelativeLayout>

    <View
        style="@style/activityOtherInformationLine"/>
    <RelativeLayout
        android:layout_marginTop="13dp"
        android:layout_marginBottom="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="所在地"
            style="@style/activityOtherInformationType"/>
        <TextView
            android:text="四川-哪？"
            style="@style/activityOtherInformationDetail" />
    </RelativeLayout>

    <View
        style="@style/activityOtherInformationLine"/>

    <RelativeLayout
        android:layout_marginTop="13dp"
        android:layout_marginBottom="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="简介"
            style="@style/activityOtherInformationType"/>
        <TextView
            android:text="这个人很懒。。。"
            style="@style/activityOtherInformationDetail" />
    </RelativeLayout>

    <View
        style="@style/activityOtherInformationLine"/>

    <RelativeLayout
        android:layout_marginTop="13dp"
        android:layout_marginBottom="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="职业"
            style="@style/activityOtherInformationType"/>
        <TextView
            android:text="学生"
            style="@style/activityOtherInformationDetail" />
    </RelativeLayout>

    <View
        style="@style/activityOtherInformationLine"/>

    <RelativeLayout
        android:layout_marginTop="13dp"
        android:layout_marginBottom="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="登记"
            style="@style/activityOtherInformationType"/>
        <TextView
            android:text="LV.MAX"
            style="@style/activityOtherInformationDetail" />
    </RelativeLayout>

    <View
        style="@style/activityOtherInformationLine"/>

    <RelativeLayout
        android:layout_marginTop="13dp"
        android:layout_marginBottom="12dp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="horizontal">
        <TextView
            android:text="注册日期"
            style="@style/activityOtherInformationType"/>
        <TextView
            android:text="2222 2-22"
            style="@style/activityOtherInformationDetail" />
    </RelativeLayout>

    <View
        style="@style/activityOtherInformationLine"/>


</LinearLayout>
