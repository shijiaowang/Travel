<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="@dimen/x46">
    <TextView
        android:gravity="center"
        android:id="@+id/tv_type"
        android:layout_marginLeft="@dimen/x20"
        android:text="骑行"
        android:textSize="@dimen/x13sp"
        android:textColor="@color/color4d4d4d"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x45" />
    <com.yunspeak.travel.ui.view.FontsIconTextView
        android:id="@+id/tv_ok"
        android:gravity="center"
        android:layout_marginRight="@dimen/x21"
        android:layout_alignParentRight="true"
        android:text="@string/activity_aite_ok"
        android:textSize="@dimen/x13sp"
        android:textColor="@color/colordbdbdb"
        android:layout_width="wrap_content"
        android:layout_height="@dimen/x45"/>
    <View
        android:layout_alignParentBottom="true"
        android:background="@color/meLine"
        android:layout_width="match_parent"
        android:layout_height="@dimen/x1"/>

</RelativeLayout>
