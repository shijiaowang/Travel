<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/x110"
    android:layout_height="@dimen/x110">
    <com.facebook.drawee.view.SimpleDraweeView
        android:id="@+id/iv_picture"
        android:layout_width="@dimen/x110"
        android:layout_height="@dimen/x110" />
    <View
        android:id="@+id/iv_picture_1"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />
    <com.yunspeak.travel.ui.view.FontsIconTextView
        android:id="@+id/tv_select"
        android:text="@string/activity_aite_ok"
        android:layout_marginTop="@dimen/x5"
        android:layout_marginRight="@dimen/x5"
        android:layout_gravity="right"
        android:textColor="@color/colorb5b5b5"
        android:textSize="@dimen/x15sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />



</FrameLayout>
