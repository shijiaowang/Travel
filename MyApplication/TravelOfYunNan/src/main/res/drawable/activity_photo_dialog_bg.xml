<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <size
                android:width="300dp"
                android:height="120dp"/>
            <solid
                android:color="@color/colorFAFAFA" />
            <stroke
                android:width="3dp"
                android:color="@color/otherTitleBg" />
            <corners android:radius="10dp" />
        </shape>
    </item>
    <item
          android:top="40dp"
        >
        <shape android:shape="rectangle">
            <size
                android:width="120dp"
                android:height="200dp"/>
            <stroke

                android:width="3dp"
                android:color="@color/otherTitleBg" />
            <solid android:color="@color/colorFAFAFA" />
        </shape>
    </item>
    <item
        android:top="30dp"
        android:left="3dp"
        android:right="3dp"
        android:bottom="3dp"
        >
        <shape android:shape="rectangle">
            <size
                android:width="120dp"
                android:height="20dp"/>
            <solid android:color="@color/colorFAFAFA" />
        </shape>
    </item>
</layer-list>
