<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--activity的一些布局样式-->
    <style name="oneHeightMatchLine">
        <item name="android:background">@color/meLine</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">1dp</item>
        </style>
    <!--公用button-->
    <style name="commonButtonNext">
        <item name="android:id">@id/bt_next</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:text">下一步</item>
        <item name="android:background">@color/otherTitleBg</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">@color/colorFAFAFA</item>
    </style>



    <!--认证页面-->
    <style name="activityIdentityAuthenticationRl">
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
    </style>
    <style name="activityIdentityAuthenticationText">
        <item name="android:layout_marginLeft">35dp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/color323232</item>
    </style>
    <style name="activityIdentityAuthenticationIcon" parent="activityIdentityAuthenticationText">
        <item name="android:layout_marginLeft">0dp</item>
    </style>
    <style name="activityIdentityAuthenticationCursor" parent="activityIdentityAuthenticationIcon">
        <item name="android:textColor">@color/color969696</item>
        <item name="android:text">@string/activity_message_center_cursor</item>
        <item name="android:layout_alignParentRight">true</item>

    </style>
    <style name="activityIdentityAuthenticationType" parent="activityIdentityAuthenticationCursor">
        <item name="android:layout_marginRight">5dp</item>
        <item name="android:layout_alignParentRight">false</item>
    </style>
    <!--首页搜索页面-->
    <style name="activityHomeSearchLl">
        <item name="android:orientation">horizontal</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">match_parent</item>
    </style>

    <style name="activityHomeSearchText">
        <item name="android:textSize">13sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>

    <!--消息中心-->
    <style name="activityMessageCenterLl">
        <item name="android:gravity">center_vertical</item>
        <item name="android:orientation">horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">45dp</item>
        <item name="android:background">@color/colorFAFAFA</item>
        <item name="android:paddingLeft">13dp</item>
        <item name="android:paddingRight">13dp</item>
    </style>
    <style name="activityMessageCenterIconText">
        <item name="android:textSize">20sp</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="activityMessageCenterText" parent="activityMessageCenterIconText">
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/color8c8c8c</item>
        <item name="android:layout_marginLeft">10dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="activityMessageCenterCursor" parent="activityMessageCenterIconText">
        <item name="android:textSize">11sp</item>
        <item name="android:textColor">#7b7b7b</item>
    </style>



    <style name="XScrollViewListViewCommon" parent="commonListViewStyle">
        <item name="android:focusable">false</item>
        <item name="android:focusableInTouchMode">false</item>
    </style>
  <!--旅行计划-->
    <style name="activityTravelsPlanRl">
        <item name="android:layout_marginTop">15dp</item>
        <item name="android:gravity">center_vertical</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="activityTravelsPlanTv">
        <item name="android:textColor">@color/color323232</item>
        <item name="android:textSize">16sp</item>
        <item name="android:layout_marginLeft">22dp</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
    </style>
    <style name="activityTravelsPlanDes" parent="activityTravelsPlanTv">
        <item name="android:layout_marginLeft">53dp</item>
    </style>
    <style name="activityTravelsPlanTvTime" parent="activityTravelsPlanTv">
        <item name="android:textColor">@color/color646464</item>
        <item name="android:textSize">15sp</item>
        <item name="android:layout_marginLeft">0dp</item>

    </style>
    <style name="activityTravelsPlanTvTimeSelect">
        <item name="android:textColor">@color/color323232</item>
        <item name="android:textSize">15sp</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/activity_travels_plan_time_bg</item>
        <item name="android:layout_width">106dp</item>
        <item name="android:layout_height">23dp</item>
    </style>
    <style name="activityCostSettingText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/color646464</item>
    </style>


    <!--一起玩详情页-->
    <style name="activityTogetherDetail">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">9sp</item>
        <item name="android:textColor">@color/colorb5b5b5</item>
    </style>
    <!--目的地选择-->
    <style name="activitySelectDestinationText">
        <item name="android:layout_width">0dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">12sp</item>
        <item name="android:textColor">@color/colorC1c1c1</item>
    </style>
    <!--添加景点-->
    <style name="activityAddDestinationLl">
    <item name="android:layout_width">match_parent</item>
    <item name="android:layout_height">wrap_content</item>
    <item name="android:orientation">horizontal</item>
    <item name="android:layout_marginLeft">22dp</item>
</style>
    <style name="activityAddDestinationIcon">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:textSize">14sp</item>
        <item name="android:layout_gravity">start</item>
        <item name="android:textColor">@color/color313131</item>

    </style>
    <style name="activityAddDestinationText" parent="activityAddDestinationIcon">
        <item name="android:layout_marginLeft">5dp</item>
    </style>
    <style name="activityAddDestinationEd">
        <item name="android:layout_marginLeft">23dp</item>
        <item name="android:background">@drawable/activity_travels_plan_time_bg</item>
    </style>
    <!--pop-->
    <style name="appointPopText">
        <item name="android:layout_width">wrap_content</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:gravity">center</item>
        <item name="android:textSize">13sp</item>
        <item name="android:textColor">@color/colorFAFAFA</item>
        <item name="android:layout_centerVertical">true</item>
    </style>

</resources>
